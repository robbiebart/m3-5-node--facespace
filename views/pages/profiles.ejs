<%- include('../partials/header') %>
<div class="profile-page--banner"></div>
<div class="profile-page__user">
    <img class="profile-page__user--avatar" src="<%= user.avatarUrl %>" />
    <h2><%= user.name %></h2>
</div>
    <div class="profile-page__friends">
        <h3><%= user.name %>'s Friends</h3>
        <ul class="friend, profile-page__friends--list">
            <% friends.forEach((friend) => { %>
                <li class="friend"> <p> <%= friend.name %> </p>
                    <img src="<%= friend.avatarUrl %>" /> </li>
                    <% }) %>
                </ul>
            </div>

        <%- include('../partials/footer') %>